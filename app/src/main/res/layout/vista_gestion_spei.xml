                <TextView
                    android:text="TextView"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:id="@+id/clave_p_b"
                    android:textColor="@color/md_black_1000"
                    android:visibility="invisible"/>

            </TableRow>

        </LinearLayout>

    </RelativeLayout>

</LinearLayout>