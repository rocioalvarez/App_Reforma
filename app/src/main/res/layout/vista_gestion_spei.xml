                    android:textAlignment="textEnd"/>

            </TableRow>

            <TableRow
                android:id="@+id/tableRow2a"
                android:layout_width="match_parent"
                android:layout_height="4dp">

                <TextView
                    android:text="TextView"
                    android:layout_width="0dp"
