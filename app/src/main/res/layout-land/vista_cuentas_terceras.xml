<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.DrawerLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_drawer_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="@bool/fitsSystemWindows">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/status_bar_kitkat_height"
            android:background="?colorPrimary"/>

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/status_bar_lollipop_height"
            android:background="?colorPrimaryDark"/>

        <TableRow
            android:id="@+id/tableRow1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="9dp">

        <TextView
            android:text="Cuenta Retiro:"
            android:layout_width="130dp"
            android:layout_height="wrap_content"
            android:id="@+id/textView8"
            android:textStyle="normal|bold"
            android:textColor="@android:color/background_dark"
            android:textSize="15sp"
            android:layout_marginStart="15dp"/>

        <Spinner
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/cuenta"
            android:hint="cuenta"
            android:textColor="@android:color/background_dark"
            android:layout_marginEnd="40dp"/>

            <Spinner
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:id="@+id/idcliente"
                android:hint="cuenta"
                android:textColor="@android:color/background_dark"

                android:visibility="invisible"/>
        </TableRow>

        <TableRow
            android:id="@+id/tableRow"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:text="Nombre:"
                android:layout_width="65dp"
                android:layout_height="wrap_content"
                android:id="@+id/textView1"
                android:textStyle="normal|bold"
                android:textColor="@android:color/background_dark"
                android:textSize="15sp"
                android:layout_marginStart="15dp"/>

            <EditText
                android:layout_width="245dp"
                android:layout_height="wrap_content"
                android:inputType="textPersonName"
                android:ems="10"
                android:id="@+id/nombre"
                android:textColor="@android:color/background_dark"
                android:textSize="15sp"
                android:enabled="false"/>

            <TextView
                android:text="RFC:"
                android:layout_width="60dp"
                android:layout_height="wrap_content"
                android:id="@+id/textVie"
                android:textStyle="normal|bold"
                android:textSize="15sp"
                android:textColor="@android:color/background_dark"/>

            <EditText
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:inputType="textPersonName"
                android:ems="10"
                android:id="@+id/rfc"
                android:textColor="@android:color/background_dark"
                android:textSize="15sp"
                android:enabled="false"
                android:layout_marginEnd="40dp"/>
        </TableRow>

        <TableRow
            android:id="@+id/tableRow2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp">

        <TextView
            android:text="Cuenta Deposito:"
            android:layout_width="130dp"
            android:layout_height="wrap_content"
            android:id="@+id/textView17"
            android:textStyle="normal|bold"
            android:textColor="@android:color/background_dark"
            android:textSize="15sp"
            android:layout_marginStart="15dp"/>

        <Spinner
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/cbeneficiario"
            android:hint="cuenta beneficiario"
            android:textColor="@android:color/background_dark"
            android:layout_marginEnd="40dp"/>
        </TableRow>

        <TableRow
            android:id="@+id/tableRow3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:text="Beneficiario:"
                android:layout_width="90dp"
                android:layout_height="wrap_content"
                android:id="@+id/textView12"
                android:textStyle="normal|bold"
                android:textColor="@android:color/background_dark"
                android:textSize="15sp"
                android:layout_marginStart="15dp"/>

            <EditText
                android:layout_width="200dp"
                android:layout_height="wrap_content"
                android:inputType="textPersonName"
                android:ems="10"
                android:id="@+id/beneficiario"
                android:hint="Ingrese beneficiario"
                android:textColor="@android:color/background_dark"
                android:textSize="15sp" />

            <TextView
                android:text="RFC:"
                android:layout_width="60dp"
                android:layout_height="wrap_content"
                android:id="@+id/textView14"
                android:textStyle="normal|bold"
                android:textSize="15sp"
                android:textColor="@android:color/background_dark"
                android:layout_marginStart="10dp"/>

            <EditText
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:inputType="textPersonName"
                android:ems="10"
                android:id="@+id/rfcbene"
                android:textColor="@android:color/background_dark"
                android:textSize="15sp"
                android:enabled="false"
                android:layout_marginEnd="40dp"/>
        </TableRow>

        <Button
            android:text="VER DATOS"
            android:layout_width="129dp"
            android:layout_height="37dp"
            android:id="@+id/btn_datos"
            android:textStyle="normal|bold"
            android:layout_marginStart="260dp"/>

        <TableRow
            android:id="@+id/tableRow5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

        <TextView
            android:text="Concepto:"
            android:layout_width="75dp"
            android:layout_height="wrap_content"
            android:id="@+id/textView11"
            android:textStyle="normal|bold"
            android:textSize="15sp"
            android:textColor="@android:color/background_dark"
            android:layout_marginStart="15dp"/>

        <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/concepto"
            android:hint="Ingrese concepto"
            android:textColor="@android:color/background_dark"
            android:layout_marginEnd="40dp"
            android:textSize="15sp"/>
        </TableRow>

        <TableRow
            android:id="@+id/tableRow6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

        <TextView
            android:text="Cantidad:"
            android:layout_width="75dp"
            android:layout_height="wrap_content"
            android:id="@+id/textView7"
            android:textStyle="normal|bold"
            android:textSize="15sp"
            android:textColor="@android:color/background_dark"
            android:layout_marginStart="15dp"/>

        <EditText
            android:layout_width="180dp"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:id="@+id/cantidad"
            android:hint="Ingrese cantidad"
            android:textColor="@android:color/background_dark"
            android:textSize="15sp" />

            <TextView
                android:text="Clave OperaciÃ³n:"
                android:layout_width="120dp"
                android:layout_height="wrap_content"
                android:id="@+id/textView18"
                android:textStyle="normal|bold"
                android:textSize="15sp"
                android:textColor="@android:color/background_dark"
                android:layout_marginStart="3dp"/>

            <EditText
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ems="10"
                android:id="@+id/cvoperacion"
                android:hint="Ingrese clave de operacion"
                android:textColor="@android:color/background_dark"
                android:inputType="textPassword"
                android:layout_marginEnd="40dp"
                android:textSize="15sp" />


        </TableRow>

        <Button
            android:text="ENVIAR"
            android:id="@+id/btn_Fin"
            android:textColor="@color/md_amber_50"
            android:background="@drawable/shape"
            android:layout_marginStart="250dp"
            android:layout_height="40dp"
            android:layout_width="150dp"
            android:backgroundTint="@color/md_blue"
            android:layout_marginTop="5dp"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/resultado"
            android:visibility="invisible"/>

        <EditText
            android:visibility="invisible"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/idbeneficiario" />

        <EditText
            android:visibility="invisible"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/RFC" />

        <EditText
            android:layout_width="wrap_content"
            android:layout_height="0dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/iva"
            android:hint="Ingresa iva"
            android:textSize="14sp"
            android:visibility="invisible"
            android:layout_alignParentStart="true" />
    </LinearLayout>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/status_bar_margin_top">

        <FrameLayout
            android:id="@+id/fragment"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
        </FrameLayout>

        <android.support.v7.widget.Toolbar
            android:id="@+id/toolbar"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:background="?attr/colorPrimary"
            android:elevation="4dp"
            app:popupTheme="@style/ThemeOverlay.AppCompat.Light"
            app:theme="@style/ToolbarTheme" />


    </FrameLayout>

    <android.support.design.widget.NavigationView
        android:id="@+id/navigation_view"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_gravity="start"
        android:fitsSystemWindows="@bool/fitsSystemWindows"
        app:headerLayout="@layout/navigation_drawer_header"
        app:menu="@menu/navigation_drawer_menu"
        app:theme="@style/NavigationViewTheme" />


</android.support.v4.widget.DrawerLayout>